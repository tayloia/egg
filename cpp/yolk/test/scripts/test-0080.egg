print(type.of(object.property));
///>object
/// WIBBLE >object.Property

var v = {};
print(v);
///>{}
print(type.of(v));
///>object

var p = object.property.ref(v, "i");
print(type.of(p));
///>any?*
print(v);
///>{}

*p = "hello";
print(v);
///>{i:"hello"}

v.i = "goodbye";
print(v);
///>{i:"goodbye"}

print(*p);
///>goodbye
