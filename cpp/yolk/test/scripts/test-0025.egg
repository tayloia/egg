int fibonacci(int n) {
  assert(n >= 0);
  if (n < 2) {
    return n;
  }
  return fibonacci(n - 1) + fibonacci(n - 2);
}
try {
  print(fibonacci(10));
} catch (any exception) {
  print("CAUGHT:", exception);
}
