int... countdown(int counter) {
  while (counter > 0) {
    yield counter;
    --counter;
  }
}
assert(type.of(countdown) == "int...(int)");
var f = countdown(5);
assert(type.of(f) == "int...");
while (var i = f()) {
  assert(type.of(i) == "int");
  print(i);
}
