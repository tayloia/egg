object v = {};
print(v);
///>{}

print(object.property.set(v, "i", 123));
///>void
print(v);
///>{i:123}

print(object.property.get(v, "i"));
///>123

print(object.property.mut(v, "a", "!!=", 10));
///>void
print(v);
///>{i:123,a:10}

print(object.property.mut(v, "a", "--"));
///>10
print(v);
///>{i:123,a:9}

print(object.property.mut(v, "a", "=", "text"));
///>9
print(v);
///>{i:123,a:"text"}

var p = object.property.ref(v, "i");
print(type.of(p));
///>any?*
print(*p);
///>123
*p = 321;
print(v);
///>{i:321,a:"text"}

print(object.property.del(v, "i"));
///>321
print(v);
///>{a:"text"}
