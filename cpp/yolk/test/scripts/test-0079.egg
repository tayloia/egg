object v = {};
print(v);
///>{}

print(object.set(v, "i", 123));
///>void
print(v);
///>{i:123}

print(object.get(v, "i"));
///>123

print(object.mut(v, "a", "!!=", 10));
///>void
print(v);
///>{i:123,a:10}

print(object.mut(v, "a", "--"));
///>10
print(v);
///>{i:123,a:9}

print(object.mut(v, "a", "=", "text"));
///>9
print(v);
///>{i:123,a:"text"}

/* WIBBLE
  var p = object.ref(v, "i");
  print(type.of(p));
  ///>int*
  print(*p);
  ///>123
  *p = 321;
  print(v);
  ///>{i:321,a:"text"}
*/

print(object.del(v, "i"));
///>123
print(v);
///>{a:"text"}
